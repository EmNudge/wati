{
    "comments": {
        "lineComment": ";;",
        "blockComment": [ "(;", ";)" ]
    },
    "brackets": [
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["(", ")"],
        ["\"", "\""]
    ],
    "surroundingPairs": [
        ["(", ")"],
        ["\"", "\""]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^((?!;;).)*(\\(func .*|loop.*|if.*|block.*)$",
        "decreaseIndentPattern": "^((?!.*?\/\\*).*\\*\/)?\\s*(?:\\)|end).*$"
    }
}