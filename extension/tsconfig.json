{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"imports": {
			"#vscode": "vscode"
		},
		"target": "es6",
		"lib": [
			"ES2019",
			"ES2020.String",
			"ES2019.Array"
		],
		"moduleResolution": "NodeNext",
		"rootDir": "src",
		"checkJs": true,
		"strict": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
